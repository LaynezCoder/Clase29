<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="338.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="herenciasuperthis.FXMLDocumentController">
   <children>
      <VBox alignment="CENTER" fillWidth="false" prefHeight="400.0" prefWidth="600.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Animal" />
            <ComboBox fx:id="comboBoxAnimal" prefWidth="150.0" />
            <Label text="Patas" />
            <TextField fx:id="txtPatas" />
            <Label text="Ojos" />
            <TextField fx:id="txtOjos" />
            <Label text="Sonido del animal" />
            <TextField fx:id="txtSonido" />
            <VBox fx:id="piojosContainer" alignment="CENTER" fillWidth="false" prefHeight="113.0" prefWidth="600.0" spacing="10.0">
               <children>
                  <CheckBox fx:id="checkPiojos" mnemonicParsing="false" text="Piojos" />
               </children>
            </VBox>
            <Button mnemonicParsing="false" onAction="#onCrearAnimal" text="Crear animal" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
